{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "ESNext",
        "target": "ES5",
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true
    },
    "ts-node": {
        "compilerOptions": {
          "module": "CommonJS"
        }
      }
}
